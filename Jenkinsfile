pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building the new application'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deployed the new application'
            }
        }
        stage('new_patch') {
            steps {
                echo 'fixed the bugs and patch applied'
            }
        }
    }
}
